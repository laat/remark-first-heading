{
  "name": "remark-first-heading",
  "version": "1.0.0",
  "main": "lib/remark-first-heading",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel src/. -d lib/. --ignore=spec.js",
    "test": "npm run test:readme && npm run test:spec",
    "test:readme": "readme-assert",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13 src/**/*.spec.js",
    "lint": "standard src/**/*.js",
    "prebuild": "npm run clean",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "http://laat.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/remark-first-heading.git"
  },
  "dependencies": {
    "mdast-util-to-string": "^1.0.1"
  },
  "peerDependencies": {
    "remark": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "mocha-tap13": "0.0.3",
    "readme-assert": "^2.0.1",
    "remark": "^4.1.2",
    "standard": "^6.0.8"
  },
  "files": [
    "lib"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
